{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "NODE_PATH=dist node -r dotenv/config dist/server.js",
    "build-and-start": "npm run build && npm run start",
    "watch": "nodemon --exec 'npm run build-and-start || exit 1'",
    "db-migrate-prod": "prisma migrate deploy --schema=../db/schema.prisma",
    "db-migrate-dev": "prisma migrate dev --schema=../db/schema.prisma",
    "start-production": "npm run db-migrate-prod && NODE_ENV=production npm run build-and-start",
    "standard": "standard",
    "postinstall": "patch-package"
  },
  "nodemonConfig": {
    "delay": "1000"
  },
  "engineStrict": true,
  "overrides": {
    "secure-password": {
      "sodium-native": "3.3.0"
    }
  },
  "engines": {
    "node": "^18.12.0",
    "npm": "^8.19.2"
  },
  "dependencies": {"cookie-parser": "~1.4.6",
  "cors": "^2.8.5",
  "express": "~4.18.1",
  "morgan": "~1.10.0",
  "@prisma/client": "4.5.0",
  "jsonwebtoken": "^8.5.1",
  "secure-password": "^4.0.0",
  "dotenv": "16.0.2",
  "helmet": "^6.0.0",
  "patch-package": "^6.4.7",
  "uuid": "^9.0.0",
  "lodash": "^4.17.21",
  "baseui": "12.2.0",
  "jquery": "3.4.0",
  "js-yaml": "2.0.0",
  "puppeteer": "1.12.0",
  "react-markdown": "^8.0.5",
  "safer-eval": "1.3.3",
  "styletron-engine-atomic": "1.5.0",
  "styletron-react": "6.1.0",
  "systeminformation": "5.0.0"
},
  "devDependencies": {"nodemon": "^2.0.19",
  "standard": "^17.0.0",
  "prisma": "4.5.0",
  "typescript": "^4.8.4",
  "@types/node": "^18.11.9",
  "@tsconfig/node18": "^1.0.1"
}
}
